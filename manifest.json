{
  "manifest_version": 2,

  "name": "Github Search",
  "description": "This extension adds more functionality to Github search page",
  "version": "0.1",
  "background": {"page": "background.html"},
  "page_action": {
    "name": "github search",
    "default_icon": "icon.png"
  },
  "permissions": [ "https://github.com/*", "storage" ],
   "content_scripts": [{
        "matches": ["<all_urls>","https://github.com/*","https://www.github.com/search/*","https://github.com/search/*"],
        "js": ["jquery-1.11.2.js","content.js"]
    }]
}
